{
  "name": "@gravitate-health/lens-tool-bundler",
  "version": "0.2.1",
  "description": "Tool to bundle from raw lens to a FHIR Standard Lens",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/Gravitate-Health/lens-tool-bundler",
  "bugs": "https://github.com/Gravitate-Health/lens-tool-bundler/issues",
  "repository": "Gravitate-Health/lens-tool-bundler",
  "license": "Apache License 2.0",
  "author": "Universidad PolitÃ©cnica de Madrid (UPM)",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "lens-tool-bundler": "./bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "pnpm run build && oclif manifest",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "pnpm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "pnpm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "bin": "lens-tool-bundler",
    "commands": "./dist/commands",
    "dirname": "lens-tool-bundler",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "bundle": {
        "description": "Bundle lenses from a source code format to JSON"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5.4.54",
    "inquirer": "^9.2.23",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.11.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
